{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel",
    "promise"
  ],
  "env": {
    "browser": true
  },
  "globals": {
  },
  "rules": {
    "key-spacing": 0,
    "jsx-quotes": [2, "prefer-single"],
    "max-len": 0,
    "camelcase": 0,
    "object-curly-spacing": [2, "always"]
  }
}
